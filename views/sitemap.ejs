<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>http://www.opendatanetwork.com/</loc>
        <changefreq>monthly</changefreq>
    </url>
    <url>
        <loc>http://www.opendatanetwork.com/join-open-data-network</loc>
        <changefreq>monthly</changefreq>
    </url>
<% regions.forEach(function(region) { %>
    <url>
        <loc>http://www.opendatanetwork.com/region/<%= region.child_id %>/<%= region.child_name.replace(/ /g, '_').replace(/,/g, '') %></loc>
        <changefreq>monthly</changefreq>
    </url>
<% }); %>
</urlset>