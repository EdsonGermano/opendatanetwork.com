
<% questions.forEach((question, index) => { %>
    <li class="question <%= index > 1 ? 'collapsible collapsed' : '' %>">
        <a href="<%= navigate.variable(question.id).url() %>">
            What is the
            <span class="question-variable"><%= question.name %></span>?
        </a>
    </li>
<% }); %>

