<% include header %>

<div class="content">
    <table>
        <tr>
            <th>Sector</th>
            <th>Housing</th>
            <th>Restaurant Inspections</th>
            <th>Transit</th>
            <th>Health</th>
            <th>Crime</th>
        </tr>
      <% results.forEach(function(result) { %>
        <tr>
            <td><a href="<%= result.portal_url %>"><%= result.portal_title %></a></td>
            <td><a href="<%= result.portal_url %>?q=housing&sortBy=relevance&utf8=%E2%9C%93"><%= result.housing %></a></td>
            <td><a href="<%= result.portal_url %>?q=restaurant%20inspections&sortBy=relevance&utf8=%E2%9C%93"><%= result.restaurant_inspections %></a></td>
            <td><a href="<%= result.portal_url %>?q=transit&sortBy=relevance&utf8=%E2%9C%93"><%= result.transit %></a></td>
            <td><a href="<%= result.portal_url %>?q=health&sortBy=relevance&utf8=%E2%9C%93"><%= result.health %></a></td>
            <td><a href="<%= result.portal_url %>?q=crime&sortBy=relevance&utf8=%E2%9C%93"><%= result.crime %></a></td>
        </tr>
      <% }); %>
    </table>
</div>

<% include footer %>